Tarea2 Ejercicio 13 */

#include <stdio.h>

int main(){
 int arr[100] ;
int n,i;
int sum=0;

printf("cuantos? "); scanf("%d",&n);

for(i=0;i<n;i++){
    printf("dato %d: ",i) ;
    scanf("%d",&arr[i]);
    sum = sum + arr[i] ;
}

printf("suma = %d",sum);

return 0;
}
