/*Fernanda Arely Moreno De Le√≥n
Tarea 2 Ejercicio 15*/



#include <stdio.h>

int main(){
int arr[100];
int n,i;
int pos=0,neg=0,cer=0;

printf("n: ");
scanf("%d",&n);

for(i=0;i<n;i++){
   printf("dato: ");
   scanf("%d",&arr[i]);

   if(arr[i]>0) pos++;
   else if(arr[i]<0) neg++;
   else cer++;
}

printf("pos %d\n",pos);
printf("neg %d\n",neg);
printf("ceros %d",cer);

return 0;
}
