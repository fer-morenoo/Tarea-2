#include <stdio.h>

int main(){

int a,b;

printf("dos nums: ");
scanf("%d %d",&a,&b) ;

if(b!=0 && a%b==0){
   printf("%d es divisible entre %d", a, b);
}
else if(a!=0 && b%a==0){
    printf("%d es divisible entre %d", b, a);
}
else{
 printf("no son divisbles") ;
}

return 0;
}
